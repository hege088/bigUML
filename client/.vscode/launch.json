{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Debug) UML GLSP VSCode Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "${workspaceFolder}/workspace",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/uml-vscode-integration/extension"
      ],
      "outFiles": ["${workspaceFolder}/packages/uml-vscode-integration/extension/lib/*.js"],
      "sourceMaps": true,
      "env": {
        "UML_GLSP_SERVER_DEBUG": "true",
        "UML_GLSP_SERVER_PORT": "5007",
        "UML_MODEL_SERVER_DEBUG": "true",
        "UML_MODEL_SERVER_PORT": "8081"
      }
    },
    {
      "name": "(Debug) UML GLSP VSCode Extension 2",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "${workspaceFolder}/workspace",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/uml-vscode-integration/extension"
      ],
      "outFiles": ["${workspaceFolder}/packages/uml-vscode-integration/extension/lib/*.js"],
      "sourceMaps": true,
      "env": {
        "UML_GLSP_SERVER_DEBUG": "true",
        "UML_GLSP_SERVER_PORT": "5017",
        "UML_MODEL_SERVER_DEBUG": "true",
        "UML_MODEL_SERVER_PORT": "8091"
      }
    },
    {
      "name": "(Production) UML GLSP VSCode Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "${workspaceFolder}/workspace",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/uml-vscode-integration/extension"
      ],
      "outFiles": ["${workspaceFolder}/packages/uml-vscode-integration/extension/lib/*.js"],
      "sourceMaps": true,
      "env": {
        "UML_GLSP_SERVER_DEBUG": "false",
        "UML_GLSP_SERVER_LOGGING": "false",
        "UML_MODEL_SERVER_DEBUG": "false",
        "UML_MODEL_SERVER_LOGGING": "false"
      }
    }
  ]
}
